{
  "name": "@atomist/tree-path",
  "version": "0.2.0",
  "description": "Atomist path expression support",
  "author": {
    "name": "Atomist",
    "email": "support@atomist.com",
    "url": "https://atomist.com/"
  },
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/atomist/tree-path-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atomist/tree-path-ts.git"
  },
  "bugs": {
    "url": "https://github.com/atomist/tree-path-ts/issues"
  },
  "keywords": [
    "atomist",
    "parser",
    "path-expressions"
  ],
  "dependencies": {
    "@atomist/microgrammar": "^0.9.1",
    "@types/lodash": "^4.14.116",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^8.1.3",
    "mocha": "^3.5.3",
    "power-assert": "^1.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.1",
    "typedoc": "^0.8.0",
    "typescript": "2.8.3",
    "typescript-formatter": "^6.1.0"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor -q -n exit -e ts -x npm -- run test",
    "build": "npm run lint && npm run compile && npm test",
    "clean": "npm run clean-js ; rm -rf *-v8.log profile.txt build",
    "clean-js": "find src test -type f -name '*.js' -print0 | xargs -0 rm -f",
    "compile": "tsc -p .",
    "distclean": "npm run clean ; rm -rf node_modules",
    "fmt": "tsfmt --replace",
    "lint": "tslint --format verbose --project . --exclude '{build,node_modules}/**' '**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha --compilers ts:espower-typescript/guess 'test/**/!(*atomist.config).ts'",
    "testone": "mocha --compilers ts:espower-typescript/guess \"test/**/${TEST:-*.ts}\"",
    "typedoc": "typedoc --mode modules --excludeExternals"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  }
}
